<ul class="mainMenu">
<%= render 'layouts/admin/cms/menu', current: :content -%>
</ul>

<ul class="contentMenu">
<%= content_tag('li') { link_to "カテゴリ種別一覧", gp_category_category_types_path, class: current == :category ? 'current' : '' } -%>
<%= content_tag('li') { link_to "テンプレート一覧", gp_category_templates_path, class: current == :template ? 'current' : '' } -%>
<%= content_tag('li') { link_to "モジュール一覧", gp_category_template_modules_path, class: current == :template_module ? 'current' : '' } -%>
<%= content_tag('li') { link_to "カテゴリ種別設定", gp_category_content_settings_path, class: current == :setting ? 'current' : '' } if @content.editable? -%>
</ul>
