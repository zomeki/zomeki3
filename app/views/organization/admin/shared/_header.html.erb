<ul class="mainMenu">
<%= render 'layouts/admin/cms/menu', current: :content -%>
</ul>

<ul class="contentMenu">
<%= content_tag('li') { link_to "組織一覧", organization_groups_path(content: @content), class: current == :group ? 'current' : '' } -%>
<%= content_tag('li') { link_to "組織設定", organization_content_settings_path, class: current == :setting ? 'current' : '' } if @content.editable? -%>
</ul>
