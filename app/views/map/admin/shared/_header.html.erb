<ul class="mainMenu">
<%= render 'layouts/admin/cms/menu', current: :content -%>
</ul>

<ul class="contentMenu">
<%= content_tag('li') { link_to "マーカー一覧", map_markers_path, class: current == :marker ? 'current' : '' } -%>
<%= content_tag('li') { link_to "マーカー設定", map_category_types_path, class: current == :category ? 'current' : '' } -%>
<%= content_tag('li') { link_to 'マップ設定', map_content_settings_path, class: current == :setting ? 'current' : '' } if @content.editable? -%>
</ul>
