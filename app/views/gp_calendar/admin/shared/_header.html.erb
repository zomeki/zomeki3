<ul class="mainMenu">
<%= render 'layouts/admin/cms/menu', current: :content -%>
</ul>

<ul class="contentMenu">
<%= content_tag('li') { link_to "イベント一覧", gp_calendar_events_path, class: current == :event ? 'current' : '' } -%>
<%= content_tag('li') { link_to "休日一覧", gp_calendar_holidays_path, class: current == :holiday ? 'current' : '' } -%>
<%= content_tag('li') { link_to "イベント設定", gp_calendar_content_settings_path, class: current == :setting ? 'current' : '' } if @content.editable? -%>
</ul>
