<div id="doc_style_tags" title="置き換えテキスト" style="display: none;">
  <table>
    <tr>
      <th>タイトル（リンクなし）</th><td>@title@</td>
    </tr>
    <tr>
      <th>タイトル（リンクあり）</th><td>@title_link@</td>
    </tr>
    <%- unless request.path_info.include?("calendar_list_style") -%>
    <tr>
      <th>サブタイトル</th><td>@subtitle@</td>
    </tr>
    <tr>
      <th>開催日</th><td>@hold_date@</td>
    </tr>
    <tr>
      <th>概要</th><td>@summary@</td>
    </tr>
    <tr>
      <th>組織</th><td>@unit@</td>
    </tr>
    <%- end -%>
    <tr>
      <th>カテゴリ</th><td>@category@</td>
    </tr>
    <%- unless request.path_info.include?("calendar_list_style") -%>
    <tr>
      <th>カテゴリタイプ</th><td>@category_type_カテゴリタイプid@</td>
    </tr>
    <tr>
      <th>添付画像（リンクなし）</th><td>@image@</td>
    </tr>
    <tr>
      <th>添付画像（リンクあり）</th><td>@image_link@</td>
    </tr>
    <tr>
      <th>備考</th>
      <td>@note@</td>
    </tr>
    <%- end -%>
  </table>
</div>
